{
    "$schema": "http://json.schemastore.org/tsconfig",
    "extends": "../../super.tsconfig.json",
    "compilerOptions": {
        "lib": [
            "ES2020",
            "DOM",
            "DOM.Iterable"
        ],
        "module": "ES2020"
    },
    "include": [
        "./**/*"
    ],
    "exclude": [
        "./**/preload.ts",
        "./**/*.preload.ts"
    ],
    "references": [
        {
            "path": ".preload.tsconfig.json"
        },
        {
            "path": "../common/.tsconfig.json"
        }
    ]
}
